import("stdfaust.lib");
freq = hslider("freq", 440,50,2000,0.01);
screamer = hslider("screamer", 440,50,2000,0.01);
choose = nentry("choose", 0, 0, 1, 1); 

osc2 = os.osc(screamer * 1.5) * 0.2;
oscScream = os.osc(screamer + os.osc(5)*20) * 0.3 + osc2;

oscNormal = os.osc(freq) * 0.3;

process = select2(choose, oscNormal, oscScream);